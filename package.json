{
  "name": "react-ssr-firebase-hosting",
  "version": "1.0.0",
  "description": "A starter for Server Side Rendered React apps on Firebase Hosting",
  "main": "index.js",
  "scripts": {
    "webpack": "node_modules/.bin/webpack",
    "babel": "node_modules/.bin/babel src -d functions/src && node_modules/.bin/babel api -d functions/api && node_modules/.bin/babel index.js -d functions",
    "build:server": "rm -rf functions/src && yarn babel",
    "build:dev": "node_modules/.bin/webpack && yarn build:server",
    "build:prod": "node_modules/.bin/webpack -p && yarn build:server",
    "serve": "node_modules/.bin/firebase serve --only functions,hosting",
    "deploy:ssr": "node_modules/.bin/firebase use default && node_modules/.bin/firebase deploy"
  },
  "keywords": [
    "react",
    "firebase",
    "ssr"
  ],
  "author": "Huyghe Pierre",
  "license": "ISC",
  "dependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-transform-modules-commonjs": "^7.1.0",
    "@material-ui/core": "^3.2.0",
    "compression": "^1.7.3",
    "connected-react-router": "^4.5.0",
    "history": "^4.7.2",
    "isomorphic-fetch": "^2.2.1",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-jss": "^8.6.1",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-optional-catch-binding": "^7.0.0",
    "@babel/plugin-transform-async-to-generator": "^7.1.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.4",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "esm": "^3.0.84",
    "express": "^4.16.3",
    "firebase-admin": "^6.0.0",
    "firebase-functions": "^2.0.5",
    "firebase-tools": "^5.0.1",
    "html-webpack-plugin": "^2.30.1",
    "webpack": "^3.5.5"
  }
}
